<table style="width:50%">
    <h2>Person</h2>
    <tr>
        <th>Id</th>
        <th>dvgId</th>
        <th>name</th>
        <th>vorname</th>
        <th>addresse</th>
        <th>email</th>
    </tr>
    {{#each this.data.person}}
    <tr>
        <td align="center">{{this.id}}</td>
        <td align="center">{{this.dvgId}}</td>
        <td align="center">{{this.name}}</td>
        <td align="center">{{this.vorname}}</td>
        <td align="center">{{this.adresse}}</td>
        <td align="center">{{this.email}}</td>
    </tr>
    {{/each}}
</table>

<table style="width:50%">
    <h2>Landesverband</h2>
    <tr>
        <th>Id</th>
        <th>name</th>
    </tr>
    {{#each this.data.landesverband}}
    <tr>
        <td align="center">{{this.id}}</td>
        <td align="center">{{this.name}}</td>
    </tr>
    {{/each}}
</table>


<table style="width:50%">
    <h2>Kreisverband</h2>
    <tr>
        <th>Id</th>
        <th>name</th>
        <th>landesverband</th>
    </tr>
    {{#each this.data.kreisverband}}
    <tr>
        <td align="center">{{this.id}}</td>
        <td align="center">{{this.name}}</td>
        <td align="center">{{this.landesverbandId}}</td>
    </tr>
    {{/each}}
</table>